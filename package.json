{
  "name": "mina",
  "version": "0.0.0",
  "description": "Node.js Addons Example #9",
  "main": "test.js",
  "private": true,
  "gypfile": true,
  "scripts": {
    "pretty": "prettier --write **/*",
    "_start": "electron src/main.js",
    "start": "nodemon --watch src --exec electron src/main.js src/index.html src/style.css",
    "build": "npm install",
    "build-verbose": "npm install --verbose",
    "test": "node test.js",
    "update:whisper": "git submodule update --remote whisper.cpp",
    "node": "node main.js",
    "clean": "node-gyp clean",
    "rebuild": "node-gyp rebuild"
  },
  "dependencies": {
    "bindings": "*",
    "electron": "^22.0.0",
    "node-addon-api": "*",
    "clipboardy": "^2.3.0",
    "node-microphone": "^0.1.5",
    "wav": "^1.0.2"
  },
  "devDependencies": {
    "@types/audioworklet": "^0.0.36",
    "eslint": "^8.30.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4"
  }
}
